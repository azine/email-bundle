{
    "name": "efestosw/email-bundle",
    "type": "symfony-bundle",
    "description": "Azine EmailBundle fork allowing custom implementation of users.",
    "keywords": ["email", "newsletter", "notification", "updates", "email web view", "mailgun", "email open tracking"],
    "homepage": "https://github.com/efestosw/email-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrea Crocco",
            "email": "a.crocco@efesto.software"
        }
    ],
    "config": {
        "process-timeout": 590,
        "github-protocols": ["https", "git", "ssh"],
        "github-domains": ["github.com"]
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.6,>=2.6",
        "symfony/console": "~2.6,>=2.6",
        "symfony/finder": "~2.6,>=2.6",
        "symfony/twig-bundle": "~2.6,>=2.6",
        "swiftmailer/swiftmailer": ">=4.2.0,~5.0",
        "doctrine/orm": "~2.0,>=2.0",
        "monolog/monolog": "~1.6,>=1.6.0",
        "twig/extensions": "~1.0",
        "ramsey/uuid": "~3.2"
    },
    "autoload": {
        "psr-0": { "Azine\\EmailBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Azine/EmailBundle"
}
